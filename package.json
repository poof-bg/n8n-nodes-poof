{
  "name": "@poof-bg/n8n-nodes-poof",
  "version": "1.0.4",
  "description": "n8n nodes for Poof background removal API",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "poof",
    "background-removal",
    "image-processing"
  ],
  "license": "MIT",
  "homepage": "https://poof.bg",
  "author": {
    "name": "Poof",
    "email": "support@poof.bg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/poof-bg/n8n-nodes-poof"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint nodes credentials package.json --ext .ts,.json",
    "lintfix": "eslint nodes credentials package.json --ext .ts,.json --fix",
    "test": "npm run lint",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/PoofApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Poof/Poof.node.js"
    ]
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.5",
    "gulp": "^4.0.2",
    "jsonc-eslint-parser": "^2.4.2",
    "n8n-workflow": "^1.22.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
